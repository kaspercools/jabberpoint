<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1">
    <center>
        <VBox prefWidth="412.0">
            <children>
                <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Selecteer de te presenteren inhoud">
                    <font>
                        <Font name="Verdana" size="24.0"/>
                    </font>
                    <VBox.margin>
                        <Insets bottom="10.0" left="2.0" right="2.0" top="2.0"/>
                    </VBox.margin>
                </Text>
                <ListView fx:id="sequenceListView" prefHeight="200.0" prefWidth="200.0"/>
                <Button fx:id="selectButton" mnemonicParsing="false" prefHeight="27.0" prefWidth="430.0"
                        text="Selecteer">
                    <VBox.margin>
                        <Insets bottom="20.0" left="40.0" right="40.0" top="20.0"/>
                    </VBox.margin>
                </Button>
            </children>
            <BorderPane.margin>
                <Insets bottom="25.0" left="25.0" right="25.0" top="10.0"/>
            </BorderPane.margin>
        </VBox>
    </center>
</BorderPane>
